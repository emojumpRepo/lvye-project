{
  "name": "lvye-project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "deploy:frontend": "node script/windows/deploy-frontend.mjs",
    "deploy:backend": "node script/windows/deploy-backend.mjs",
    "deploy:all": "npm run deploy:backend && npm run deploy:frontend",
    "build:backend": "mvn clean package -DskipTests",
    "build:frontend": "cd yudao-ui/lvye-project-frontend && pnpm --filter @lvye/admin build && pnpm --filter @lvye/web build",
    "build:all": "npm run build:backend && npm run build:frontend",
    "release": "node scripts/release.mjs",
    "release:patch": "node scripts/quick-release.mjs --patch",
    "release:minor": "node scripts/quick-release.mjs --minor",
    "release:major": "node scripts/quick-release.mjs --major",
    "release:auto": "node scripts/quick-release.mjs --patch"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "chalk": "^5.6.2",
    "readline-sync": "^1.4.10",
    "ssh2": "^1.17.0"
  }
}
